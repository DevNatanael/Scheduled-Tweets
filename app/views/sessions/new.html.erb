<h1>Fazer Login</h1>
<%= form_with url: sign_in_path do |form|%>
  <div class="mb-3">
    <%= form.label :email%>
    <%= form.text_field :email, class: "form-control",placeholder: "teste@gmail.com" %>
  </div>
  <div class="mb-3">
    <%= form.label :password%>
    <%= form.password_field :password , class: "form-control"%>
    <%= link_to 'Esqueceu sua senha?', password_reset_path %>
  </div  >
  <div class="mb-3">
    <%= form.submit "Logar", class:"btn btn-primary"%>
  </div>
<%end%>
